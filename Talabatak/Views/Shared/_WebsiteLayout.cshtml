@{
    var Language = Request.QueryString["lang"];
    Layout = null;
}
<!doctype html>
<html lang="en">
<head>
    <title>@(Language != null && Language.ToLower() == "ar" ? "ايجبت اونلاين" : "Egypt Online App")</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Mobland - Mobile App Landing Page Template">
    <meta name="keywords" content="Talabatak, order, restaurants, app, pharmacy, ios, android, responsive">

    <!-- Font -->
    <link rel="dns-prefetch" href="//fonts.googleapis.com">
    <link href="https://fonts.googleapis.com/css?family=Rubik:300,400,500" rel="stylesheet">

    <!-- Bootstrap CSS -->
    <link href="~/Content/Website/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Themify Icons -->
    <link href="~/Content/Website/css/themify-icons.css" rel="stylesheet" />
    <link href="~/Content/Website/css/owl.carousel.min.css" rel="stylesheet" />
    <!-- Owl carousel -->
    <!-- Main css -->
    <link rel="icon" type="image/png" href="~/Content/assets/imgs/logo.png">
    <link href="~/Content/Website/css/style.css" rel="stylesheet" />
    @if (Language != null && Language.ToLower() == "ar")
    {
        <link href="https://fonts.googleapis.com/css?family=Cairo&display=swap" rel="stylesheet">
        <style>
            body {
                font-family: 'Cairo', sans-serif !important;
            }
        </style>
    }
</head>

<body data-spy="scroll" data-target="#navbar" data-offset="30">
    @Html.Partial("_WebsiteNavbar")
    @RenderBody()
    @Html.Partial("_WebsiteFooter")
    <!-- jQuery and Bootstrap -->
    <script src="~/Content/Website/js/jquery-3.2.1.min.js"></script>
    <script src="~/Content/Website/js/bootstrap.bundle.min.js"></script>
    <!-- Plugins JS -->
    <script src="~/Content/Website/js/owl.carousel.min.js"></script>
    <!-- Custom JS -->
    <script src="~/Content/Website/js/script.js"></script>
</body>
</html>
