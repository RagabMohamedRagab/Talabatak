@model Talabatak.Models.Domains.Complaint
<div class="modal fade" id="ContactUsMessageModal" tabindex="-1" role="dialog" aria-labelledby="ContactUsMessageModal" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">@(!string.IsNullOrEmpty(Model.UserId) ? Model.User.Name : "غير مشترك")</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <p><b>رقم الهاتف:</b> @(!string.IsNullOrEmpty(Model.UserId) ? Model.User.PhoneNumber : "غير مشترك")</p>
                <hr />
                <h5>الرساله:</h5>
                <q>@Model.Message</q>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">اغلاق</button>
            </div>
        </div>
    </div>
</div>

